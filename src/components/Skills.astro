<section id="skills" class="py-20">
  <div class="container mx-auto px-4">
    <h2 class="text-3xl font-bold text-center mb-12">My Skills</h2>

    {
      Object.entries(skills).map(([category, skillList]) => (
        <div class="mb-12 group">
          <h3 class="text-xl font-semibold text-center mb-8 text-gray-700 dark:text-gray-300">
            {category}
          </h3>
          <div class="flex flex-wrap justify-center gap-8 md:gap-12">
            {skillList.map((skill) => (
              <div
                class:list={[
                  "flex flex-col items-center gap-2 w-24 transition-all duration-300",
                  {
                    "opacity-60 grayscale group-hover:opacity-100 group-hover:grayscale-0":
                      category === "Next to Learn",
                  },
                ]}
              >
                <div
                  class="w-16 h-16 flex items-center justify-center text-gray-600 dark:text-gray-400"
                  set:html={skill.icon}
                />
                <span class="font-medium text-nowrap">{skill.name}</span>
              </div>
            ))}
          </div>
        </div>
      ))
    }
  </div>
</section>
